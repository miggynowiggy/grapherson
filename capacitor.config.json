{
  "appId": "com.grapherson",
  "appName": "Grapherson",
  "bundledWebRuntime": true,
  "webDir": "dist",
  "npmClient": "yarn",
  "plugins": {
    "Keyboard": {
      "resize": "body"
    },
    "SplashScreen": {
      "showSpinner": true,
      "splashFullScreen": true,
      "splashImmersive": true,
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "large"
    }
  },
  "backgroundColor": "#00B882"
}